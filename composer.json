{
    "name": "n5s/front-end-accounts",
    "description": "Account-related forms on the front end of your WordPress website.",
    "license": "MIT",
    "version": "1.0",
    "keywords": ["wordpress", "plugin", "user", "front-end", "profile"],
    "homepage": "https://github.com/chrisguitarguy/Front-End-Accounts",
    "type": "wordpress-plugin",
    "authors": [
        {
            "name": "Christopher Davis",
            "email": "chris@classicalguitar.org",
            "homepage": "http://christopherdavis.me",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/chrisguitarguy/Front-End-Accounts/issues",
        "source": "https://github.com/chrisguitarguy/Front-End-Accounts"
    },
    "require": {
        "composer/installers": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Chrisguitarguy\\FrontEndAccounts\\": "inc"
        }
    },
    "require-dev": {
        "rector/rector": "^0.18.6"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
